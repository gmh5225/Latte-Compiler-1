###### Integers arthmetical operations ######

int main(){
    int x = 30;
    int y = 14;
    int result = x + y + 7; //30 + 14 + 7 = 51 

    printInt(result);
    printInt(9 + 7 - 3); // 13
    printInt(-2 -3 -9 + 1); // -13
    
    int z;
    int a;
    a = -100;
    x = 90;
    result = z + a + x; // 0 - 100 + 90 = -10
    
    printInt(result);

    int b = 3; 
    int c = 9;
    int d = 2;
    printInt(b*c*3*d*1); // 3 * 9 * 2 * 3 = 9*9*2 = 162
    printInt(d*d*d); // 2^3 = 8
    printInt(1*1*1*1*b); // 3
    
    int zero;
    zero--;
    printInt(zero); // -1
    zero--;
    printInt(zero); // -2

    printInt(3/2); // 1
    printInt(100/4); // 25
    printInt(63/7/3); // 3

    printInt(9 % 2); // 1
    printInt(20 % 6); // 2
    printInt(10243232 % 2); // 0
    return 0;
}