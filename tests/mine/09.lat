###### Nested loops ######

int main() {
    int i = 1-1;
    int j = 1-1;
    while(i < 4){
        
        j = i;
        while(j < 4){
            printInt(j); 
            j++;
        }
        i++;
    }
    //  i = 0 j = 0,1,2,3
    //  i = 1 j = 1,2,3
    //  i = 2 j = 2,3
    //  i = 3 j = 3
    return 0;
}
