###### String concatenation ######

int main() {
    string s1 = "Hello";
    string s2 = "world";
    string result = s1 + " " + s2;
    printString(result); // Hello world
    
    string s;
    printString(result+s); // Hello world

    printString(result+"!"); // Hello world!

    printString(s1+s1+s1); //HelloHelloHello

    result = result + "!";
    printString(result); // Hello world!

    return 0;
}
